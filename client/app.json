{
  "expo": {
    "runtimeVersion": "1.0.0",
    "name": "Sportsmatch",
    "slug": "sportmatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/smicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/smsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/251b90c8-ec2b-4e23-a18f-23ab9affdbc4"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aythenapps.sportsmatch",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/smiconmain.png",
        "backgroundColor": "#000"
      },
      "versionCode": 1,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.aythenapps.sportsmatch"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      ["expo-apple-authentication"],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2e24e316-402e-462f-8799-ee82d86792bd"
      }
    },
    "owner": "azschiaffino",
    "scheme": [
      "sportsmatch",
      "fb955537832791728"
    ]
  }
}
